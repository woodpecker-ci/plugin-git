{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":maintainLockFilesWeekly"],
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "matchManagers": ["gomod", "dockerfile"],
      "labels": ["dependencies"]
    },
    {
      "matchManagers": ["woodpecker"],
      "labels": ["build"]
    }
  ]
}
